<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.keelean.citest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <!--<instrumentation android:label="CI Test" android:targetPackage="com.keelean.citest" android:name="android.test.InstrumentationTestRunner" />-->
    <instrumentation android:targetPackage="com.keelean.citest" android:name="android.test.InstrumentationTestRunner" />
</manifest>
